                "type": "object",
                "required": [
                    "data"
                ],
                "properties": {
                    "branch": {
                        "type": "string",
                        "example": "main"
                    },
                    "data": {
                        "$ref": "#/components/schemas/models.JSONData"
                    },
                    "name": {
                        "type": "string",
                        "example": "My Article"
                    },
                    "schemaVersion": {
                        "type": "string",
                        "example": "v1"
                    },
                    "scope": {
                        "type": "string",
                        "example": "organization"
                    }
